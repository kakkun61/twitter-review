span.signin-with-google {
    display: inline-block;
    width: 191px;
    height: 46px;
    background-image: url(@{StaticR images_btn_google_signin_light_normal_web_png});
}
span.signin-with-google:hover {
    background-image: url(@{StaticR images_btn_google_signin_light_focus_web_png});
}
span.signin-with-google:active {
    background-image: url(@{StaticR images_btn_google_signin_light_pressed_web_png});
}
